		                <name>outputdatatable</name>
		                <value>madlibtestresult.lda_toy_outdata</value>
		            </parameter>
		            <parameter>
                        <name>vocsize</name>
		                <value>20</value>
                    </parameter>
                    <parameter>
                        <name>topicnum</name>
		                <value>5</value>
                    </parameter>
                    <parameter>
                        <name>iternum</name>

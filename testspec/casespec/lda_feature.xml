<?xml version='1.0' encoding='UTF-8'?>
<test_suites>
    <test_type>feature</test_type>
    <multi_test_suites>
        <algorithm>lda</algorithm>
        <methods>
            <method>
                <name>lda_train</name>
                <parameter>
                    <name>benchmark</name>
                    <value>TemplateExecutor</value>
                </parameter>
            </method>
            <method>
                <name>lda_predict</name>
                <parameter>
                    <name>benchmark</name>
                    <value>TemplateExecutor</value>
                </parameter>
            </method>
        </methods>
        <test_suite>
	        <name>lda_basic_function_nips</name>
            <comments>
                Test the functionality of lda over the nips dataset.
            </comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>lda_train</name>
	            <parameters>
                    <parameter>
		                <name>datatable</name>
		                <value>madlibtestdata.lda_nips_corpus</value>
                    </parameter>
                    <parameter>
		                <name>modeltable</name>
		                <value>madlibtestresult.lda_model</value>
                    </parameter>
                    <parameter>
		                <name>outputdatatable</name>
		                <value>madlibtestresult.lda_outdata</value>
                    </parameter>
                    <parameter>
                        <name>vocsize</name>
		                <value>12420</value>
                    </parameter>
    	        </parameters>
	    	    <tear_down>
                </tear_down>
	        </method>
            <method>
                <name>lda_predict</name>
	            <parameters>
                    <parameter>
		                <name>datatable</name>
		                <value>madlibtestdata.lda_nips_corpus</value>
                    </parameter>
                    <parameter>
		                <name>modeltable</name>
		                <value>madlibtestresult.lda_model</value>
                    </parameter>
                    <parameter>
		                <name>outputdatatable</name>
		                <value>madlibtestresult.lda_outdata</value>
                    </parameter>
	            </parameters>
		        <tear_down>
                    DROP TABLE IF EXISTS madlibtestresult.lda_outdata;DROP TABLE IF EXISTS madlibtestresult.lda_model;
                </tear_down>
	        </method>
        </test_suite>
        <test_suite>
	        <name>lda_basic_function_nytimes</name>
            <comments>
                Test the functionality of lda over the nytimes dataset.
            </comments>
            <execute_rate>1</execute_rate>
            <method>
                <name>lda_train</name>
	            <parameters>
                    <parameter>
		                <name>datatable</name>
		                <value>madlibtestdata.lda_nytimes_corpus</value>
                    </parameter>
                    <parameter>
		                <name>modeltable</name>
		                <value>madlibtestresult.lda_model</value>
                    </parameter>
                    <parameter>
		                <name>outputdatatable</name>
		                <value>madlibtestresult.lda_outdata</value>
                    </parameter>
                    <parameter>
                        <name>vocsize</name>
		                <value>122660</value>
                    </parameter>
    	        </parameters>
	    	    <tear_down>
                </tear_down>
	        </method>
            <method>
                <name>lda_predict</name>
	            <parameters>
                    <parameter>
		                <name>datatable</name>
		                <value>madlibtestdata.lda_nytimes_corpus</value>
                    </parameter>
                    <parameter>
		                <name>modeltable</name>
		                <value>madlibtestresult.lda_model</value>
                    </parameter>
                    <parameter>
		                <name>outputdatatable</name>
		                <value>madlibtestresult.lda_outdata</value>
                    </parameter>
	            </parameters>
		        <tear_down>
                    DROP TABLE IF EXISTS madlibtestresult.lda_outdata;DROP TABLE IF EXISTS madlibtestresult.lda_model;
                </tear_down>
	        </method>
        </test_suite>
    </multi_test_suites>
</test_suites>
